{
    "routes": [
        {
            "src": "/",
            "dest": "/api"
        }
    ],
    "functions": {
        "api/**/*.ts": {
            "includeFiles": "api/**/*.ts"
        }
    },
    "crons": [
        {
            "path": "/api/cron",
            "schedule": "0 0 * * *"
        }
    ]
}